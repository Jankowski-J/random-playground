{
  "manifest_version": 2,
  "name": "Trello highlight",
  "version": "1.0",

  "description": "Highlights your cards on Trello.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.trello.com/*"],
      "js": ["jquery-3.3.1.min.js", "trello-highlight.js"]
    }
  ]
}